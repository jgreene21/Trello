<hr />
<ul>
<% @board.lists.each do |list|%>
<li><%= list.name %> - <%= link_to "Edit Name", edit_board_list_path(@board, list) %></li>
<ul>
<%= link_to "Add Task", new_list_task_path(list)%></li>
<% list.tasks.each do |task|%>
<li><%= "#{task.priority} - "%><%= task.bullet %>
<%= link_to "edit", edit_list_task_path(list, task)%>
</li>

<% end %>
</ul>
<% end %>
</ul>